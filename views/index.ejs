<section id="freequote" style="border: 0px solid red;">

  <div class="container-fluid" style="padding: 0px 0px;">

    <div class="row">

      <div class="thumbnail" style="padding: 0px 0px; background: #fff;border: 0px solid red;">


        <div class="col-xs-12 col-sm-12 col-md-6" style="padding: 0px 0px;">
          <div class="thumbnail" style="padding-top: 23px; background: #EEEEEE;border: 0px solid red;box-shadow: none;">

            <img src="assets/img/contactAlbertaBroker900x700.jpg" alt="my alberta broker - life Insurance" class="img-responsive" style="display: block;margin: 0 auto;" />
           <!--  <h2 style="color: #fff;background: #045F82;"> Contact Info </h2>
            <div class="form">
              <h4 style="color: #fff;">Phone: +1 (403) 714-1510</h4>
              <h4 style="color: #fff;">Email:</h4>
              <h4 style="color: #fff;">Facebook:</h4>
            </div> -->
          </div>
        </div>

        <div class="col-xs-12 col-sm-12 col-md-6" style="padding: 0px 0px;padding-right: 10px;border: 0px solid red;">

            <h2 style="color:#333; background:#EEEEEE;"> Free Quote </h2>

              <form class="form" method="post" action="/" role="form" id="contactform" style="border: 0px solid red;padding: 0px 30px;width: 100%;">


                <div class="row" style="width: 100%;padding: 0px 0px;">

                    <div class="col-sm-12 col-md-6">
                      <p style="color:#000;">Insurance type</p>
                      <select class="form-control input-md fontawesome-select" id="type" name="type">
                          <option value="Life Insurance">&#xf1ae;LIFE</option>
                          <option value="Health Insurance">&#xf004;HEALTH</option>
                          <option value="Travel Insurance">&#xf072;TRAVEL</option>
                          <option value="Home Insurance">&#xf015;HOME</option>
                          <option value="Auto Insurance">&#xf1b9;AUTO</option>
                      </select>
                    </div>

                    <div class="col-sm-12 col-md-6">
                        <p style="color:#000;">Name:</p>
                        <input class="form-control input-md" id="fname" placeholder="First and Last Name" name="name">
                    </div>

                </div>

                <div class="row" style="width: 100%;">
                    <div class="col-sm-12 col-md-4">
                        <p style="color:#000;">Email:</p>
                        <input type="email" class="form-control input-md" id="femail" placeholder="Email Address"  data-error="The email address is invalid" name="email">
                    </div>

                    <div class="col-sm-12 col-md-8">
                        <p style="color:#000;">Phone #: (format: xxx-xxx-xxxx)</p>
                        <input id="fphone" name="phone" type="text" placeholder="Phone" class="form-control input-md">
                    </div>
                </div>

                <div class="row" style="width: 100%;padding: 0px 0px;">

                    <div class="col-sm-12 col-md-3">

                        <p style="color:#000;">Send Via</p>
                        <select class="form-control input-md fontawesome-select" name="selectopt" id="selectopt">
                          <option value="">--</option>
                          <option value="contactus">Email</option>
                          <option value="sms">SMS</option>
                        </select>
                    </div>

                </div>

                <div class="row" style="width: 100%;">
                    
                    <div class="col-sm-12 col-md-12" style="margin-bottom: 30px;">
                      <p style="color:#000;">Message</p>
                      <textarea style="width: 100%;" name="message" rows="4" class="form-control"></textarea>
                    </div>
                </div>


                <div class="row" style="margin-bottom: 50px;">
                    <div class="col-sm-12 col-md-12">
                      <button type="submit" class="btn btn-info" style="float: right;"><i class="fa fa-envelope" aria-hidden="true" style="font-size: 2em;"></i>Send</button>
                    </div>
                </div>

              </form>

        </div>



      </div>

    </div>

  </div>
    
</section>


